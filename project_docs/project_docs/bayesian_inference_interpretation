    if t < ART_start:
        # Acute ‚Üí chronic transition
        xi_t = xi_acute + (xi_chronic - xi_acute) * (1 - exp(-k_transition * t))
    else:
        # ART normalization
        t_art = t - ART_start
        xi_t = xi_chronic - (xi_chronic - xi_healthy) * (1 - exp(-k_ART * t_art))

